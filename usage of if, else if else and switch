#include<iostream>
using namespace std;
int main()
{
  //how to use if else condition
  int marks;
  cin>>marks;
  if(marks>90)
  {
    cout<<"A"<<endl;
  }
  else if(marks>80 && marks<90)
  {
    cout<<"B"<<endl;
  }
  else if(marks>70 && marks<80)
  {
    cout<<"C"<<endl;
  }
  else
  {
    cout<<"pass"<<endl;
  }
  //how to use switch case condition
  int b;
  cin>>b;
  switch(b)
  {
    case 1:
      cout<<"1"<<endl;
      break;
    case 2:
      cout<<"1"<<endl;
      break;
    default:
      cout<<"If nothing works by default it works"<<endl;
  }
    
}
